{
	"name": "web-scraper-analyzer",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"server": "node server/index.js",
		"dev:all": "concurrently \"npm run server\" \"npm run dev\" \"cd ./mobile && npm start\"",
